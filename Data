local CrashChance = math.random(1, 10)

if CrashChance == "1" then
game.Players.LocalPlayer:Kick("Figit Executer ran into an error connecting to Roblox servers. Code: 0HD:{Rbxl.Launch(functions(game:GetPlayerName():TransferToHttp(https://servers.figit.club))}")
end
